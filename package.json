{
  "name": "@studyplus/boron-ui",
  "version": "0.0.18",
  "description": "React Component Library for Studyplus for School",
  "main": "dist/index.cjs.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf ./dist && npm run build-variable-css",
    "build": "rollup -c && npm run copy-config",
    "build-variable-css": "style-dictionary build --config ./style-dictionary.config.js && prettier --write src/styles/variables.scss",
    "copy-config": "cpx ./src/config/tailwind.base.config.js ./dist/",
    "test": "jest",
    "test:ci": "jest --ci --runInBand",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "tsc": "tsc --noEmit",
    "build-icons": "svgr -d ./src/Icon ./assets/icons --typescript"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/studyplus/boron-ui.git"
  },
  "author": "Studyplus, Inc.",
  "license": "UNLICENSED",
